<link rel="icon" type="image/png" href="https://<%= origin %>/plantszone.png">
<meta name="description" content="<%= data.description %> ">
<!-- Google / Search Engine Tags -->
<meta itemprop="name" content="<%= data.product_name %> - PlantsZone">
<meta itemprop="description" content="<%= data.description %> ">
<meta itemprop="image" content="<%= data.product_media[0].image %>">
<!-- Facebook Meta Tags -->
<meta property="og:url" content="https://<%= origin %>">
<meta property="og:type" content="website">
<meta property="og:title" content="<%= data.product_name %> - PlantsZone">
<meta property="og:description" content="<%= data.description %> ">
<meta property="og:image" content="<%= data.product_media[0].image %>">
<!-- Twitter Meta Tags -->
<meta name="twitter:card" content="summary_large_image">
<meta name="twitter:title" content="<%= data.product_name %> - PlantsZone">
<meta name="twitter:description" content="<%= data.description %> ">
<meta name="twitter:image" content="<%= data.product_media[0].image %>">